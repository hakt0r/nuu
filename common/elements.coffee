list = []
weight = []
for k,v of exports
  v.name = k
  list.push v
  weight.push v.abundance
total_weight = weight.reduce (prev, cur, i, arr) -> prev + parseFloat cur

rand = (min, max) -> Math.random() * (max - min) + min
exports.random = ->
  random_num = rand(0, total_weight)
  weight_sum = 0
  i = 0
  while i < list.length
    weight_sum += weight[i]
    weight_sum =+ (parseFloat weight_sum).toFixed(2)
    return list[i] if random_num <= weight_sum
    i++
  null

if isClient then window.Elements = exports
else global.Elements = exports